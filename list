>>> print([1 for i in range(5)])
[1, 1, 1, 1, 1]
>>> print([1*1 for i in range(5)])
[1, 1, 1, 1, 1]
>>> print([i for i in range(5)])
[0, 1, 2, 3, 4]
>>> print([i*i for i in range(5)])
[0, 1, 4, 9, 16]
>>> print([i**2 for i in range(5)])
[0, 1, 4, 9, 16]
>>> print(sum([i for i in range(5)]))
10
>>> print(sum([1 for i in range(5)]))
5
